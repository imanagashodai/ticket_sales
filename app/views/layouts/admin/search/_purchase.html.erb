<div id="search_field">
  <%= form_with local: true, url: admin_purchases_search_path, method: "get" do |f| %>
    <%= f.submit "検索" %>
    <%= f.label :s_purchases_id, "id" %>
    <%= f.number_field :s_purchases_id, value: session[:s_purchases_id] %>
    <%= f.label :s_purchases_ticket_id, "ticket_id" %>
    <%= f.select :s_purchases_ticket_id, @select_ticket, {include_blank: "未定", selected: session[:s_purchases_ticket_id]} %>
    <%= f.label :s_purchases_user_id, "user_id" %>
    <%= f.select :s_purchases_user_id, @select_user, {include_blank: "未定", selected: session[:s_purchases_user_id]} %>
    <%= f.label :s_purchases_created_at, "created_at" %>
    <%= f.date_field :s_purchases_created_at, value: session[:s_purchases_created_at] %>
    <%= f.label :s_purchases_updated_at, "updated_at" %>
    <%= f.date_field :s_purchases_updated_at, value: session[:s_purchases_updated_at] %>
  <% end %>
</div>