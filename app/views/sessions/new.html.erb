<h1>ログイン画面</h1>
<%= form_with url: login_path, local: true do |f| %>
  <div>
    <p><%= f.label :email, "Eメール" %></p>
    <p><%= f.text_field :email %></p>
  </div>
  <div>
    <p><%= f.label :password, "パスワード" %></p>
    <p><%= f.password_field :password %></p>
  </div>
  <%= f.submit "ログイン" %>
<% end %>